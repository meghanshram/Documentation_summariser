{
    "manifest_version": 3,
    "name": "Documentation Query Plugin",
    "version": "1.0",
    "description": "Ask questions about the documentation page you are viewing.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "icons/icon-128.png"
    },
    "icons": {
      "48": "icons/icon.png",
      "128": "icons/icon-128.png"
    }
  }
  